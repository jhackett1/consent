generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  id Int @id @default(autoincrement())
  name String?
  email String @unique
  password_digest String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model session {
  sid Int @id @default(autoincrement())
  sess Json
  expire DateTime
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
